<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dubbo.provider.dao.TestDao">

    <update id="updateUser">
        update users
        set age=#{age},
        NAME=#{name}
        where id=#{id}
    </update>


</mapper>